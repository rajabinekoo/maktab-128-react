{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/dtos": [
        "libs/dtos/src"
      ],
      "@app/dtos/*": [
        "libs/dtos/src/*"
      ],
      "@app/entities": [
        "libs/entities/src"
      ],
      "@app/entities/*": [
        "libs/entities/src/*"
      ],
      "@app/middlewares": [
        "libs/middlewares/src"
      ],
      "@app/middlewares/*": [
        "libs/middlewares/src/*"
      ],
      "@app/storage": [
        "libs/storage/src"
      ],
      "@app/storage/*": [
        "libs/storage/src/*"
      ],
      "@app/validators": [
        "libs/validators/src"
      ],
      "@app/validators/*": [
        "libs/validators/src/*"
      ]
    }
  }
}